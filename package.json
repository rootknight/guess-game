    "eslint": "^9.31.0",
    "eslint-config-next": "^15.4.2",
    "eslint-config-prettier": "^10.1.8",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
