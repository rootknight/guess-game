    "bcryptjs": "^3.0.2",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.2",
    "framer-motion": "^12.23.6",
    "next": "^15.4.2",
    "next-auth": "^5.0.0-beta.29",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.25.76"
