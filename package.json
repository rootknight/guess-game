{
  "name": "guss-game",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules && rm -rf .next",
    "dev": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/db/migrate.ts",
